OUTPUT_FILE:=counter
INPUT_FILE:=counter.c

.PHONEY: all clean

all: $(INPUT_FILE)
	$(CC) $(INPUT_FILE) -o $(OUTPUT_FILE)

clean:
	rm -f $(OUTPUT_FILE)